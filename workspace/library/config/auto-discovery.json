  "mcpAutoSelection": {
    "_description": "Automatic MCP selection rules based on context",
    "rules": [
      {
        "condition": "intent.includes('framework') || intent.includes('docs')",
        "mcps": [
          "context7"
        ],
        "priority": 1
      },
      {
        "condition": "intent.includes('ui') || intent.includes('component')",
        "mcps": [
          "shadcn"
        ],
        "priority": 1
      },
      {
        "condition": "intent.includes('scrape') || intent.includes('web')",
        "mcps": [
          "firecrawl",
          "exa"
        ],
        "priority": 2
      },
      {
        "condition": "intent.includes('database') || intent.includes('supabase')",
        "mcps": [
          "supabase",
          "postgres"
        ],
        "priority": 1
      },
      {
        "condition": "intent.includes('github') || intent.includes('repo')",
        "mcps": [
          "github"
        ],
        "priority": 1
      },
      {
        "condition": "intent.includes('memory') || intent.includes('recall') || intent.includes('pattern')",
        "mcps": [
          "hindsight",
          "memory"
        ],
        "priority": 1
      },
      {
        "condition": "intent.includes('diagram') || intent.includes('flowchart')",
        "mcps": [
          "mermaid"
        ],
        "priority": 1
      },
      {
        "condition": "intent.includes('figma') || intent.includes('design import')",
        "mcps": [
          "figma"
        ],
        "priority": 1
      },
      {
        "condition": "intent.includes('browser') || intent.includes('automation')",
        "mcps": [
          "playwright",
          "puppeteer"
        ],
        "priority": 2
      },
      {
        "condition": "intent.includes('python') || intent.includes('code execution')",
        "mcps": [
          "e2b"
        ],
        "priority": 2
      }
    ]
  },
