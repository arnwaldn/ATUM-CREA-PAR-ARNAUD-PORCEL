{
  "hooks": {
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File \"C:\\Claude-Code-Creation\\scripts\\notification-system.ps1\" -Type complete -Message \"Claude a termine sa tache\""
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "AskUserQuestion",
        "hooks": [
          {
            "type": "command",
            "command": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File \"C:\\Claude-Code-Creation\\scripts\\notification-system.ps1\" -Type validation -Message \"Claude attend votre reponse\""
          }
        ]
      },
      {
        "matcher": "TodoWrite",
        "hooks": [
          {
            "type": "command",
            "command": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File \"C:\\Claude-Code-Creation\\scripts\\notify-light.ps1\" -Type done"
          }
        ]
      },
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File \"C:\\Claude-Code-Creation\\scripts\\notify-light.ps1\" -Type tick"
          }
        ]
      },
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File \"C:\\Claude-Code-Creation\\scripts\\notify-light.ps1\" -Type tick"
          }
        ]
      },
      {
        "matcher": "Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "node \"C:\\Claude-Code-Creation\\scripts\\hooks\\post-edit-learn.js\""
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "node \"C:\\Claude-Code-Creation\\scripts\\hooks\\auto-retain.js\""
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "node \"C:\\Claude-Code-Creation\\scripts\\hooks\\self-healing-hook.js\""
          }
        ]
      },
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "node \"C:\\Claude-Code-Creation\\scripts\\hooks\\context-monitor.js\""
          }
        ]
      },
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "node \"C:\\Claude-Code-Creation\\scripts\\hooks\\anti-hallucination.js\""
          }
        ]
      },
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "node \"C:\\Claude-Code-Creation\\scripts\\hooks\\auto-sync-hindsight.js\""
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "powershell.exe -NoProfile -Command \"Write-Host '[ULTRA-CREATE v25.0] Memory-First Protocol Active' -ForegroundColor Cyan\""
          }
        ]
