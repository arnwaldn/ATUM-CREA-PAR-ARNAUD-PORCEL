        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "node \"C:\\Claude-Code-Creation\\scripts\\hooks\\knowledge-auto-load.js\""
          }
        ]
      },
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "node \"C:\\Claude-Code-Creation\\scripts\\hooks\\mcp-auto-router.js\""
