<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Member Sequence -->
        <record id="seq_association_member" model="ir.sequence">
            <field name="name">Association Member</field>
            <field name="code">association.member</field>
            <field name="prefix">MBR-</field>
            <field name="padding">5</field>
        </record>

        <!-- Membership Sequence -->
        <record id="seq_association_membership" model="ir.sequence">
            <field name="name">Association Membership</field>
            <field name="code">association.membership</field>
            <field name="prefix">SUB-%(year)s-</field>
            <field name="padding">4</field>
        </record>

        <!-- Event Sequence -->
        <record id="seq_association_event" model="ir.sequence">
            <field name="name">Association Event</field>
            <field name="code">association.event</field>
            <field name="prefix">EVT-%(year)s-</field>
            <field name="padding">4</field>
        </record>

        <!-- Donation Sequence -->
        <record id="seq_association_donation" model="ir.sequence">
            <field name="name">Association Donation</field>
            <field name="code">association.donation</field>
            <field name="prefix">DON-%(year)s-</field>
            <field name="padding">4</field>
        </record>

        <!-- Tax Receipt Sequence -->
        <record id="seq_association_tax_receipt" model="ir.sequence">
            <field name="name">Tax Receipt</field>
            <field name="code">association.tax.receipt</field>
            <field name="prefix">REC-%(year)s-</field>
            <field name="padding">5</field>
        </record>

        <!-- Resource Booking Sequence -->
        <record id="seq_association_resource_booking" model="ir.sequence">
            <field name="name">Resource Booking</field>
            <field name="code">association.resource.booking</field>
            <field name="prefix">BKG-</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
