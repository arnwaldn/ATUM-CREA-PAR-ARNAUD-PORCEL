<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Demo Venues -->
        <record id="venue_main_hall" model="association.venue">
            <field name="name">Main Hall</field>
            <field name="capacity">200</field>
            <field name="address">123 Association Street</field>
            <field name="city">Demo City</field>
            <field name="facilities">Stage, Audio System, Projector, Microphones</field>
        </record>

        <record id="venue_meeting_room" model="association.venue">
            <field name="name">Meeting Room A</field>
            <field name="capacity">30</field>
            <field name="address">123 Association Street</field>
            <field name="city">Demo City</field>
            <field name="facilities">Whiteboard, Projector, Conference Table</field>
        </record>

        <record id="venue_sports_center" model="association.venue">
            <field name="name">Sports Center</field>
            <field name="capacity">100</field>
            <field name="address">45 Sports Avenue</field>
            <field name="city">Demo City</field>
            <field name="facilities">Gym, Changing Rooms, Showers</field>
        </record>

        <!-- Demo Members -->
