            <field name="gender">female</field>
            <field name="occupation">Software Engineer</field>
            <field name="street">456 Oak Street</field>
            <field name="city">Demo City</field>
            <field name="zip">12345</field>
            <field name="state">active</field>
            <field name="newsletter" eval="True"/>
        </record>

        <record id="member_bob" model="association.member">
            <field name="name">Bob Smith</field>
