        <record id="member_david" model="association.member">
            <field name="name">David Brown</field>
            <field name="email">david.brown@example.com</field>
            <field name="phone">+1 555-0104</field>
            <field name="member_type_id" ref="member_type_senior"/>
            <field name="join_date" eval="(DateTime.today() - relativedelta(years=5)).strftime('%Y-%m-%d')"/>
